{
  "manifest_version": 2,
  "name": "Theme Scheduler",
  "version": "1.1",
  "description": "A theme scheduler.",
  "developer": {
    "name": "Claymont",
    "url": "https://github.com/Claymont/theme-scheduler"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "icons": {
    "32": "icons/32.png",
    "48": "icons/48.png",
    "96": "icons/96.png"
  },
  "permissions": [
    "alarms",
    "theme",
    "management"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "applications": {
    "gecko": {
      "id": "claymont@mail.com_theme-scheduler",
      "strict_min_version": "55.0"
    }
  }
}